function SimpleTabs(e) {
  var t;
  var n = function() {
    var e = this;
    this.tab;
    this.pane;
    this.setClick = function() {
      e.tab.addEventListener("click", e.showThisTab)
    };
    this.showThisTab = function() {
      if (e !== t) {
        t.pane.className = t.pane.className.replace("active-page", "");
        t.tab.className = t.tab.className.replace("active", "");
        e.pane.className = e.pane.className + " active-page";
        e.tab.className = e.tab.className + " active";
        t = e;
      }
    }
  };
  var r = e;
  var i;
  var s = r.getElementsByTagName("li");
  for (i = 0; i < s.length; ++i) {
    var o = new n;
    o.tab = s[i];
    var u = s[i].className;
    var a = u.split(" ")[0];
    o.pane = document.getElementById(a);
    o.setClick();
    if (u.indexOf("active") > -1) {
      t = o;
    }
  }
}

var currentPath = "";

function updateCurrentActiveSideMenu() {
  var crumbArray = [];
  if (document.getElementsByTagName("side-menu").length > 0) {
    var paths = window.location.pathname;
    currentPath = paths.replace(/^\/|\/$/g, '');
    if(currentPath  == "promotions/junglee-rummy-leaderboards-invites-sent"
      || currentPath  == "promotions/junglee-rummy-leaderboards-maximum-registrations"
      || currentPath  == "promotions/junglee-rummy-leaderboards-maximum-depositors"
      || currentPath  == "promotions/junglee-rummy-leaderboards-maximum-amount-wagered"){
      currentPath = "promotions/junglee-rummy-raf-leaderboards";
    }
    var elem = document.getElementById(currentPath);
    if (elem != null) {
      if (elem.className.indexOf('active') > -1 && elem.className.indexOf('selected') > -1) {
        return;
      }
    }
    var menuLinks = document.getElementsByTagName("side-menu")[0].querySelectorAll("a");
    if (menuLinks) {
      for (var i = 0; i < menuLinks.length; i++) {
        menuLinks[i].removeClassOperation("active");
        menuLinks[i].removeClassOperation("selected");
        var sibling = menuLinks[i].nextElementSibling;
        if (sibling && sibling.className.indexOf('sub-menu') > -1) {
          sibling.removeClassOperation("open");
        }
      }
    }
    if (elem) {
      if (elem.parentNode.parentNode.className.indexOf('sub-menu') == -1) {
        //main menu element clicked
        elem.addClassOperation("active");
        var sibling = elem.nextElementSibling;
        if (sibling && sibling.className.indexOf('sub-menu') > -1) {
          sibling.addClassOperation("open");
          elem.addClassOperation("selected");
        }
        crumbArray.push(elem);
      } else {
        //sub menu element clicked
        var parentElem = elem.parentNode.parentNode.parentNode.getElementsByClassName('menu_header')[0];
        parentElem.addClassOperation("active");
        var sibling = parentElem.nextElementSibling;
        if (sibling && sibling.className.indexOf('sub-menu') > -1) {
          sibling.addClassOperation("open");
        }
        elem.addClassOperation("selected");
        crumbArray.push(parentElem,elem);
      }
      if(hideRafMenu){
        if(document.getElementById("promotions/junglee-rummy-raf-leaderboards")){
          document.getElementById("promotions/junglee-rummy-raf-leaderboards").style.display = "none";
        }
      }
      else{
        if(document.getElementById("promotions/junglee-rummy-raf-leaderboards")){
          document.getElementById("promotions/junglee-rummy-raf-leaderboards").style.display = "block";
        }
      }
      updateBreadCrumbs(crumbArray);
      add_breadcrum_cta();

    }else{
      add_breadcrum_cta();
    }
  }
}

Element.prototype.addClassOperation = function(className) {
  if (this.classList && this.classList.add) {
    this.classList.add(className);
  } else {
    this.className += " " + className;
  }
};

Element.prototype.removeClassOperation = function(className) {
  if (this.classList && this.classList.remove) {
    this.classList.remove(className);
  } else {
    this.className.replace(new RegExp('(?:^|\\s)' + className + '(?:\\s|$)'), ' ');
  }
};

