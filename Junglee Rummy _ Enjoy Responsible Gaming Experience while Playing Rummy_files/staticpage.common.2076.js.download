                     }else if(date === today.getDate() && year === today.getFullYear() && month === today.getMonth()){
                         cell.classList.add("curr-date");
                     }else if((date < today.getDate() + 3) && year === today.getFullYear() && month === today.getMonth()){
                         cell.classList.add("old-date");
                     }else {

                        var selectedDate = new Date((month+1)+'/'+date+'/'+year);
                        var daysDifference = Math.abs(Math.floor(
                          (
                             Date.UTC(today.getFullYear(), today.getMonth(), today.getDate()) -
                             Date.UTC(selectedDate.getFullYear(), selectedDate.getMonth(), selectedDate.getDate())
                         )) /(1000 * 60 * 60 * 24));
                        if(daysDifference > 180){
                            cell.classList.add("old-date");
                        }

                        if(today.getDate() + 1 > daysInMonth(today.getMonth(), year)
                        || today.getDate() + 2 > daysInMonth(today.getMonth(), year)){

                            if(selectedMonth == today.getMonth()+1){
                                if(((date == ((today.getDate() + 1) - daysInMonth(today.getMonth(), year)))
                                || (date == ((today.getDate() + 2) - daysInMonth(today.getMonth(), year))))){
                                    cell.classList.add("bl-date");
                                }else if(today.getDate() + 3 > daysInMonth(today.getMonth(), year)
                                    && date == ((today.getDate() + 3) - daysInMonth(today.getMonth(), year))){
                                    cell.classList.add("sel-date");
                                }
                            }

                        }else if(today.getDate() + 3 > daysInMonth(today.getMonth(), year)
                                && date == ((today.getDate() + 3) - daysInMonth(today.getMonth(), year))){
                            cell.classList.add("sel-date");
                        }
                     }

                     cell.appendChild(cellText);
                     row.appendChild(cell);
                     date++;
                 }
             }

             tbl.appendChild(row); // appending each row into calendar body.

         }
     }
     function populateDate(elem){
        if(elem.target.innerText.length > 2){
            return
        }

         var today = new Date();
         var currentday = (today.getMonth() + 1)+'/'+today.getDate()+'/'+today.getFullYear();
         currentday = new Date(currentday);

         var dateVal = elem.target.innerText;
         // if(selectedMonth >= today.getMonth() && dateVal > today.getDate() + 2){
         var valforInp = dateVal+'/'+(selectedMonth + 1)+'/'+selectedYear;

        var selectedDate = new Date((selectedMonth + 1)+'/'+dateVal+'/'+selectedYear);
        var daysDifference = Math.abs(Math.floor(
          (
             Date.UTC(selectedDate.getFullYear(), selectedDate.getMonth(), selectedDate.getDate()) -
             Date.UTC(currentday.getFullYear(), currentday.getMonth(), currentday.getDate())
         )) /(1000 * 60 * 60 * 24));
        if(daysDifference > 180) return

        var monthDiff;
        monthDiff = (selectedDate.getFullYear() - currentday.getFullYear()) * 12;
        monthDiff -= currentday.getMonth();
        monthDiff += selectedDate.getMonth();
        monthDiff = monthDiff <= 0 ? 0 : monthDiff;

         if(monthDiff <= 6){
             document.getElementById('exclusionDate').value = valforInp;
             document.getElementById('calendarWig').style.display = 'none';
             document.getElementById('nextMonth').classList.remove('disable-btn');
             document.getElementById('prevMonth').classList.remove('disable-btn');
         }

     }

     function daysInMonth(iMonth, iYear) {
         return 32 - new Date(iYear, iMonth, 32).getDate();
     }

