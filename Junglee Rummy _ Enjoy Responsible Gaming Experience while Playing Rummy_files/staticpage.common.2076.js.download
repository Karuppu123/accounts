function validateSignupForm(username, password, email) {
  var errorMessage = "";
  var isValid = true;
  if (isInputEmpty(username) || checkInputLength(username, 4, 12)) {
    errorMessage += "Username must be 4-12 characters long." + "<br/>";
    isValid = false;
  } else if (validateSpecialCharacters(username) != "") {
    var isValidUserName = validateSpecialCharacters(username);
    errorMessage += isValidUserName;
    isValidUserName = "";
    isValid = false;
  }
  if (isInputEmpty(password) || checkInputLength(password, 6, 14)) {
    errorMessage += "Password must be 6-14 characters long." + "<br/>";
    isValid = false;
  } else if (username == password) {
    errorMessage += "Password cannot be same as username." + "<br/>";
    isValid = false;
  }
  if (isInputEmpty(email) || isValidEmail(email)) {
    errorMessage += "Please enter a valid email address." + "<br/>";
    isValid = false;
  }
  if (!isValid) {
    var errorHeading = "Registration Error";
    var errorMessage = errorMessage;
    document.getElementById("modal_error_box2").innerHTML = errorMessage;
  } else {
    return isValid;
  }

}

function getParameterByName(name, url) {
  if (!url) {
    url = window.location.href;
  }
  name = name.replace(/[\[\]]/g, "\\$&");
  var regex = new RegExp("[?&]" + name + "(=([^&#]*)|&|#|$)"),
    results = regex.exec(url);
  if (!results) return null;
  if (!results[2]) return '';
  return decodeURIComponent(results[2].replace(/\+/g, " "));
}
