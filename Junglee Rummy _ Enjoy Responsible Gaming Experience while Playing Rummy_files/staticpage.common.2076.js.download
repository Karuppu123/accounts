        }else{
          if(i == 0){
            data += "<option selected value='"+response[i].leaderboardId+"'>"+startTime+" to "+endTime+"</option>";
            selFlag = true;
          }
        }

      data += "<option value='"+response[i].leaderboardId+"'>"+startTime+" to "+endTime+"</option>";
    }
  }
  if(selFlag){
    data = data
  }else{
    data = "<option value='0' selected disabled hidden></option>" + data
  }

  elem.innerHTML = "";
  elem.insertAdjacentHTML("afterbegin", data);
}

