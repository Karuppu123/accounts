function isInputEmpty(value) {
  return (value === undefined || value === "" || value === null || value.length <= 0);
}
//Check Range of input value
function checkInputLength(value, minlength, maxlegth) {
  return (value.length < minlength || value.length > maxlegth);
}
//Check if email is valid
function isValidEmail(value) {
  var pattern = /\S+@\S+\.\S+/;
  return !(pattern.test(value));
}
//Check if username is valid Allowed characters [a-z][A-Z][0-9][.,',-,_]
function validateSpecialCharacters(value) {
  var pattern1 = /^[a-zA-Z0-9_'-\.]*$/;
  var pattern2 = /^[a-zA-Z0-9]+[a-zA-Z0-9_'-\.]*$/
  var pattern3 = /^[a-zA-Z0-9]+([a-zA-Z0-9]*([_'-\.])(\1?)(?!\2))*([a-zA-Z0-9])*$/
  var errorMessage = "";
  if (pattern1.test(value) == false) {
    return errorMessage = "Username can't have special characters.";
  } else if (pattern2.test(value) == false) {
    return errorMessage = "Username can not start with special chars.";
  } else if (pattern3.test(value.replace(/[_'.-]/g, '_')) == false) {
    return errorMessage = "2 consecutives specila char not allowed.";
  }
  return errorMessage;
}

function validateLoginForm(username, password) {
