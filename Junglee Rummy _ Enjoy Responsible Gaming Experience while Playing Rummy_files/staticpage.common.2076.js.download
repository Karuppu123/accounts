function getRequiredDateTime(date) {
  var convertedDate = new Date(date);
  return getMonthInString(convertedDate) + " " + convertedDate.getDate() + ", '" + getLastTwoDigitOfYear(convertedDate) + " " + formatHHMM(convertedDate);

}

function formatHHMM(date) {
  function formatHH(n) { if (((n<10? '0' : '') + n) == '00') return '12'; else return  ((n<10? '0' : '') + n) };
  function formatMM(n){return (n<10?'0':'') + n;}
  var h = date.getHours();
  return formatHH(h%12) + ':' + formatMM(date.getMinutes()) + ' ' + (h<12?'AM':'PM');
}

function getMonthInString(date) {
  const monthNames = ["Jan", "Feb", "Mar", "Apr", "May", "Jun",
          "Jul", "Aug", "Sep", "Oct", "Nov", "Dec"
      ];
  return monthNames[date.getMonth()];
}

function getLastTwoDigitOfYear(date) {
  return (date.getFullYear(date) + '').slice(-2)
}

