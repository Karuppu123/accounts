function isPwa() {
  var utm_source_value = unescape(window.location.search.replace(new RegExp("^(?:.*[&\\?]" + escape('utm_source').replace(/[\.\+\*]/g, "\\$&") + "(?:\\=([^&]*))?)?.*$", "i"), "$1"));
  if (utm_source_value == "homescreen") {
      sessionStorage.setItem('launchFrom', 'PWA');
  }
  if (sessionStorage.getItem('launchFrom') == 'PWA' || window.matchMedia('(display-mode: standalone)').matches) {
      return true;
  } else if ((window.matchMedia('(display-mode: browser)').matches == false) && (navigator.userAgent.indexOf("crosswalk") == -1)) {
      return true;
  } else {
      return false;
  }
}





