      event.stopImmediatePropagation()
      event.stopPropagation()
    }

     function confirmExcPopup(tournaments){
         var seldate = document.getElementById('exclusionDate').value;
         var period = document.getElementById('exclusionPeriod').innerText;

          if (period == 'Select a period' || (period == 'Custom Days' && seldate == "")) return;

          if(period != 'Custom Days') {
            var exclusionDays = period.split(" ")[0];
            var selDate = new Date(new Date().getTime() + exclusionDays*24*60*60*1000);
            seldate = selDate.getDate() +
                "/" +
                parseInt(selDate.getMonth()+ 1) +
                "/" +
                selDate.getFullYear();
          }
         var currentday = (today.getMonth() + 1)+'/'+today.getDate()+'/'+today.getFullYear();
         currentday = new Date(currentday);

         seldate = seldate.split('/');
         seldate = seldate[1]+'/'+seldate[0]+'/'+seldate[2]
         seldate = new Date(seldate);
         var numOfdays = parseInt((seldate.getTime() - currentday.getTime())/ (1000 * 3600 * 24));
         localStorage.setItem("selfExclusionEndDate", seldate.getTime())
         var timeDurtaion = numOfdays > 3 ? numOfdays+' days' : '72 hours'

         var popupString = `If you click on 'Yes', you will not be able to deposit and play cash games for ${timeDurtaion}. Are you sure?`;
         
         if(tournaments && tournaments.length){
            popupString = "You will not be able to play on {tournament_name} during self exclusion period. Please note that the entry fee/tournament tickets will not be refunded. Confirm to proceed with self exclusion"
            if(tournaments.length == 1){
                popupString = popupString.replace("{tournament_name}", tournaments[0].tournamentName)
            }else{
                popupString = popupString.replace("{tournament_name}", tournaments.length+" tournaments")
            }
         }
         
         var elem = document.getElementById('exclusionCont')
         elem.innerHTML = popupString;

         document.getElementById('confirmPopup').style.display = 'block';

     }

     function actionNo(){
         document.getElementById('exclusionDate').value = '';
         document.getElementById('confirmPopup').style.display = 'none';
     }

     function actionYes(){
         var selDate = document.getElementById('exclusionDate').value;
         if(selDate == "") {
          var exclusionDays = document.getElementById('exclusionPeriod').innerText.split(" ")[0];
          var selDate = new Date(new Date().getTime() + exclusionDays*24*60*60*1000);
          selDate = selDate.getDate() +
              "/" +
              parseInt(selDate.getMonth()+ 1) +
              "/" +
              selDate.getFullYear();
        }
         selDate = selDate.split('/');
         selDate = selDate[1]+'/'+selDate[0]+'/'+selDate[2]+' '+new Date().getHours()+':'+new Date().getMinutes()+':'+new Date().getSeconds()
         selDate = new Date(selDate).getTime();
         selDate = selDate + 60000 //adding 1 minute
        JwrExternal.fireSelfExclusion(selDate);
        actionNo();
     }

      function attemptSelfExclusion(){
        var selDate = document.getElementById('exclusionDate').value;
        if (selDate == "") return
        selDate = selDate.split('/');
        selDate = selDate[1] + '/' + selDate[0] + '/' + selDate[2] + ' ' + new Date().getHours() + ':' + new Date().getMinutes() + ':' + new Date().getSeconds()
        selDate = new Date(selDate).getTime();
        selDate = selDate + 60000 //adding 1 minute
        JwrExternal.fireAttemptSelfExclusion(selDate);
      }

