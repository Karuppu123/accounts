function FetchTerms(elm) {
  var x = new XMLHttpRequest();
  x.open("GET", "https://rummy-static-content.jungleerummy.com/Content/versioned/2.0.0.1/xml/tos_jwr.xml", true);
  x.onreadystatechange = function() {
    if (x.readyState == 4 && x.status == 200) {
      var doc = x.responseXML;
      var title = doc.getElementsByTagName("promotions")[0];
      var tag_type_length = title.getElementsByTagName("type");

      for (i = 0; i < tag_type_length.length; i++) {
        if (title.getElementsByTagName("type")[i].getAttribute('id') == elm) {
          var tag_type = title.getElementsByTagName("type")[i].textContent;
          var jwrmm1 = new jrummy.modal({
            jwr_m_footer: false,
            jwr_m_title: "Terms and conditions",
            jwr_m_content: tag_type
          });

          jwrmm1.open();
        }
      }
    }
  };
  x.send(null);
}

