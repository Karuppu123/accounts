  if(msg.startDate != "" && msg.endDate != ""){
    var sDate  = msg.startDate;
    var eDate = msg.endDate;
    var sparts = sDate.split("/");
    var eparts = eDate.split("/");
    var sdt = new Date(parseInt(sparts[2], 10),
      parseInt(sparts[1], 10) - 1,
      parseInt(sparts[0], 10));
    var edt = new Date(parseInt(eparts[2], 10),
      parseInt(eparts[1], 10) - 1,
      parseInt(eparts[0], 10));
    var sd = sdt.getDate();
    var ed = edt.getDate();
    var sdOrdinal = getOrdinalNum(sd);
    var edOrdinal = getOrdinalNum(ed);
    var sm = ['January','Febraury','March','April','May','June','July','August','September','October','November','December'][edt.getMonth()];
    var weekData  = sdOrdinal + " - " + edOrdinal + " " + sm;
    return weekData;
  }
}
function getOrdinalNum(n) {
  return n + (n > 0 ? ['th', 'st', 'nd', 'rd'][(n > 3 && n < 21) || n % 10 > 3 ? 0 : n % 10] : '');
}
