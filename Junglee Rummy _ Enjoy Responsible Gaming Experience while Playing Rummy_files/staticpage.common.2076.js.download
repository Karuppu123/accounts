function isIosDevice() {
  return (/iPad|iPhone|iPod/.test(navigator.userAgent) && !window.MSStream);
}
function isShareChat() {
  if (document.cookie.indexOf("isShareChat=true") > -1) {
      return true;
  } else {
      return false;
  }
}

//Check if device is IOS
function isIosDevice() {
  return (/iPad|iPhone|iPod/.test(navigator.userAgent) && !window.MSStream);
}

function getDeviceType() {
  if (navigator.userAgent.indexOf("freeapp") > -1)
      return 'psapp';
  else if (navigator.userAgent.indexOf("hzwebview") > -1)
      return 'hzwebview';
  else if (navigator.userAgent.indexOf("nativePortrait") > -1)
      return 'nativePortrait';
  else if (navigator.userAgent.indexOf("native") > -1)
      return 'native';
  else if (navigator.userAgent.indexOf("crosswalk") > -1)
      return 'apk';
  else if (isPwa() && !isIosDevice())
      return 'pwaa';
  else if (isPwa() && isIosDevice())
      return 'pwai';
  else if (navigator.userAgent.indexOf("iosipa") > -1)
      return 'ipa';
  else
      return 'mob';
}

var device = getDeviceType();

if (device != "native") {

  setTimeout(() => {
    var playNowImg = document.querySelector(".playNowImg");
    if(playNowImg){
      playNowImg.addEventListener("click", () => {
        if (device === 'native') {
          Print.postMessage("go_to_lobby");
        } else {
          window.location.href = "https://www.jungleerummy.com/client/lobby";
        }
      });
    }
    
  }, 1000);
 

  var menuIconOpen = document.querySelector(".nav-toggle-bar"),
    hemburgerOuter = document.querySelector(".hemburger-outer"),
    hemburgerInner = document.querySelector(".hemburger-inner"),
    closeIcon = document.querySelector(".hidehamburgerCloseIcon");
    if(menuIconOpen){
      menuIconOpen.addEventListener("click", () => {
        hemburgerOuter.style.cssText = "visibility: visible;opacity: 1;", setTimeout(() => {
          hemburgerInner.classList.add("menu-show-right")
        }, 400);
      }), closeIcon.addEventListener("click", () => {
        hemburgerInner.classList.remove("menu-show-right"), setTimeout(() => {
          hemburgerOuter.style.cssText = "visibility: hidden;opacity: 0;"
        }, 400)
      }), hemburgerInner.addEventListener("click", e => {
        e.preventDefault(), e.stopPropagation()
      }), hemburgerOuter.addEventListener("click", () => {
        hemburgerInner.classList.remove("menu-show-right"), setTimeout(() => {
          hemburgerOuter.style.cssText = "visibility: hidden;opacity: 0;"
        }, 400)
      });
    }
  
  let cntMenuList = document.querySelectorAll(".menu-content-list .has-submenu");
  if(cntMenuList){
    cntMenuList.forEach(e => {
      e.addEventListener("click", e => {
        let t = e.currentTarget;
        console.log(e.target), t.classList.contains("closed") ? (t.classList.remove("closed"), t.classList.add("opened"), t.querySelector("ul") ? t.querySelector("ul").style.display = "block" : "") : (t.classList.remove("opened"), t.classList.add("closed"), t.querySelector("ul") ? t.querySelector("ul").style.display = "block" : ""), e.stopPropagation()
      }, !1)
    });
  }
  
  let anchor = document.querySelectorAll(".menu-content-list a");
  if(anchor){
    anchor.forEach(e => {
      e.addEventListener("click", t => {
        let o = e.getAttribute("href");
        window.location.href = o, t.stopPropagation()
      }, !1)
    });
  }
 

}

