function handleTournamentLeaderboard(resp){
  
  let ranking = resp.ranking || [];
  let leaderboard_data = "";

  document.getElementById('currentLeaderboardStartTime').innerHTML = "Start: "+tournamentLeadboardDisplayDate(resp.start);
  document.getElementById('currentLeaderboardEndTime').innerHTML = "End: "+ tournamentLeadboardDisplayDate(resp.end);
  document.getElementById("user-info").style.display = "none";
  document.getElementById("leaderboard-update-at").style.display = "none";

  if(ranking.length > 0){
    for(var k in ranking){

      k = parseInt(k);

      var ouser_rank = (k+1);

      if (k % 2 == 0) {
        class_user_m = " even";
      } else {
        class_user_m = "";
      }

      switch (ouser_rank) {
        case 1:
          ouser_rank = "<div class='first-rank'></div>";
          break;
        case 2:
          ouser_rank = "<div class='second-rank'></div>";
          break;
        case 3:
          ouser_rank = "<div class='third-rank'></div>";
          break;
      }

      if(localStorage.getItem("userId") == ranking[k].userId){

        document.getElementById('i-rank').innerHTML = (k+1);
        document.getElementById('i-name').innerHTML = ranking[k].username;
        document.getElementById('i-wins').innerHTML = ranking[k].score;

        document.getElementById("user-info").style.display = "block";
      }

      leaderboard_data += "<ul id='leader-data' class='leader" + class_user_m + "'><li class='userrank'>" + ouser_rank + "</li><li class='username'>" + (ranking[k].username) + "</li><li class='totalWon'>" + (ranking[k].score) + "</li></ul>";
    }

    if(resp.updatedAt){
      document.getElementById('leaderboard-update-at').innerHTML = "Last Updated at : "+tournamentLeadboardDisplayDateTime(resp.updatedAt);
      document.getElementById("leaderboard-update-at").style.display = "block";
    }

    if (document.getElementById("leaderboard-users-data") != "undefined" && document.getElementById("leaderboard-users-data") != null) {
      document.getElementById("leaderboard-users-data").innerHTML = leaderboard_data;
    }
  }else{
    document.getElementById('leaderboard-users-data').innerHTML = "<div style='text-align: center;padding: 20px;'>Leaderboard has started but no players have yet not made it to the ranks.</div>";
  }
}

function tournamentLeadboardDisplayDate(date){
  date = new Date(date);
  return date.getDate()+"-"+getMonthInString(date)+"-"+date.getFullYear();
}

function tournamentLeadboardDisplayDateTime(date){
  date = new Date(date);
  return getMonthInString(date)+" "+date.getDate()+", "+date.getFullYear()+", "+ formatLeaderboardTime(date);
}

function formatLeaderboardTime(date){
  let hours = date.getHours();
  let meridien = "AM";
  let minutes  = (date.getMinutes() < 10) ? ("0"+date.getMinutes()) : date.getMinutes();

  if(hours > 12){
    hours = hours-12;
    meridien = "PM";
  }

  return hours+":"+minutes+" "+meridien;
}
